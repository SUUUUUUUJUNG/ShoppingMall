<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ReviewDTO">

<insert id="create" parameterType="ReviewDTO">
insert into REVIEWS (REVIEW_ID,GCODE,MEMBERID,REVIEW_TEXT,RATING,CREATED_AT,UPDATED_AT)
values (SUJUNG.REVIEWS_SEQ.nextval, #{review_id},#{gCode},#{memberId},#{review_Text},#{createdAt},#{createdAt})
</insert>

</mapper>